@model List<ComArticles.ViewModels.ArticleViewModel>

<table class="table table-striped">
    <thead>
        <tr>
            <th>#</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <th scope="row">@(i + 1)</th>
                <td>@Model[i].Title</td>
                <td>@Model[i].CreateTime</td>
                <td>@Model[i].ModifyTime</td>
            </tr>
        }
    </tbody>
</table>